<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="400.0" stylesheets="@styles/HighContrast.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
<children>
   <BorderPane prefHeight="600.0" prefWidth="400.0">
      <top>
         <Label accessibleText="New Gate" alignment="TOP_CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="600.0" text="New Gate" BorderPane.alignment="CENTER">
            <padding>
               <Insets bottom="10.0" top="10.0" />
            </padding>
         </Label>
      </top>
      <center>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
               <Label accessibleText="Destination Room" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="600.0" text="Destination Room of Gate" textAlignment="CENTER">
                  <padding>
                     <Insets left="10.0" right="10.0" />
                  </padding>
               </Label>
               <MenuButton accessibleHelp="Click and choose a destination room of gate" accessibleText="Choose Room" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Choose Room" textAlignment="CENTER" />
               <Label accessibleText="Destination Room" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="600.0" text="Direction of Gate" textAlignment="CENTER">
                  <padding>
                     <Insets left="10.0" right="10.0" />
                  </padding>
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
               </Label>
               <TextField accessibleHelp="Type in the direction of the room you want to add" accessibleText="Enter a direction" promptText="Enter a Direction">
                  <VBox.margin>
                     <Insets left="100.0" right="100.0" />
                  </VBox.margin>
               </TextField>
               <Label accessibleText="Destination Room" alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="600.0" text="Blocked by Object?" textAlignment="CENTER">
                  <padding>
                     <Insets left="10.0" right="10.0" />
                  </padding>
                  <VBox.margin>
                     <Insets top="10.0" />
                  </VBox.margin>
               </Label>
               <MenuButton accessibleHelp="Choose an object to be the key of this room" accessibleText="Choose Object" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Choose Objects" textAlignment="CENTER" />
               <ButtonBar prefHeight="40.0" prefWidth="200.0">
                 <buttons>
                   <Button accessibleHelp="press to create the gate" accessibleText="Create" mnemonicParsing="false" text="Create" />
                 </buttons>
                  <VBox.margin>
                     <Insets top="40.0" />
                  </VBox.margin>
                  <padding>
                     <Insets right="20.0" />
                  </padding>
               </ButtonBar>
            </children>
         </VBox>
      </center>
      <bottom>
         <Label accessibleHelp="Issues will be listed below" accessibleText="Issues" alignment="TOP_LEFT" prefHeight="73.0" prefWidth="600.0" text="Issues:" BorderPane.alignment="CENTER">
            <padding>
               <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
            </padding></Label>
      </bottom>
   </BorderPane>
</children></AnchorPane>
